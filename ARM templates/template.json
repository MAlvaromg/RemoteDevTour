{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "registries_SecureDevOpsACR_name": {
            "defaultValue": "SecureDevOpsACR",
            "type": "String"
        },
        "managedClusters_Secure_devops_aks_name": {
            "defaultValue": "Secure-devops-aks",
            "type": "String"
        },
        "containerGroups_securedevops_sonarqube_name": {
            "defaultValue": "securedevops-sonarqube",
            "type": "String"
        },
        "workspaces_defaultworkspace_16169bdf_109c_4eb7_9963_3d01e2352774_weu_externalid": {
            "defaultValue": "/subscriptions/16169bdf-109c-4eb7-9963-3d01e2352774/resourceGroups/defaultresourcegroup-weu/providers/microsoft.operationalinsights/workspaces/defaultworkspace-16169bdf-109c-4eb7-9963-3d01e2352774-weu",
            "type": "String"
        },
        "publicIPAddresses_56161307_afd6_48b8_9dcd_11dd561b0776_externalid": {
            "defaultValue": "/subscriptions/16169bdf-109c-4eb7-9963-3d01e2352774/resourceGroups/MC_Secure-DevOps-Tour_Secure-devops-aks_westeurope/providers/Microsoft.Network/publicIPAddresses/56161307-afd6-48b8-9dcd-11dd561b0776",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ContainerInstance/containerGroups",
            "apiVersion": "2018-10-01",
            "name": "[parameters('containerGroups_securedevops_sonarqube_name')]",
            "location": "westeurope",
            "properties": {
                "containers": [
                    {
                        "name": "[parameters('containerGroups_securedevops_sonarqube_name')]",
                        "properties": {
                            "image": "sonarqube",
                            "ports": [
                                {
                                    "protocol": "TCP",
                                    "port": 9000
                                }
                            ],
                            "environmentVariables": [],
                            "resources": {
                                "requests": {
                                    "memoryInGB": 3.5,
                                    "cpu": 2
                                }
                            }
                        }
                    }
                ],
                "restartPolicy": "Always",
                "ipAddress": {
                    "ports": [
                        {
                            "protocol": "TCP",
                            "port": 9000
                        }
                    ],
                    "ip": "20.54.131.129",
                    "type": "Public",
                    "dnsNameLabel": "[concat(parameters('containerGroups_securedevops_sonarqube_name'), 'dns')]"
                },
                "osType": "Linux"
            }
        },
        {
            "type": "Microsoft.ContainerRegistry/registries",
            "apiVersion": "2019-12-01-preview",
            "name": "[parameters('registries_SecureDevOpsACR_name')]",
            "location": "northeurope",
            "sku": {
                "name": "Standard",
                "tier": "Standard"
            },
            "properties": {
                "adminUserEnabled": true,
                "policies": {
                    "quarantinePolicy": {
                        "status": "disabled"
                    },
                    "trustPolicy": {
                        "type": "Notary",
                        "status": "disabled"
                    },
                    "retentionPolicy": {
                        "days": 7,
                        "status": "disabled"
                    }
                },
                "encryption": {
                    "status": "disabled"
                },
                "dataEndpointEnabled": false,
                "publicNetworkAccess": "Enabled"
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters",
            "apiVersion": "2020-03-01",
            "name": "[parameters('managedClusters_Secure_devops_aks_name')]",
            "location": "westeurope",
            "sku": {
                "name": "Basic",
                "tier": "Free"
            },
            "properties": {
                "kubernetesVersion": "1.15.11",
                "dnsPrefix": "[concat(parameters('managedClusters_Secure_devops_aks_name'), '-dns')]",
                "agentPoolProfiles": [
                    {
                        "name": "agentpool",
                        "count": 2,
                        "vmSize": "Standard_DS2_v2",
                        "osDiskSizeGB": 128,
                        "maxPods": 110,
                        "type": "VirtualMachineScaleSets",
                        "orchestratorVersion": "1.15.11",
                        "nodeLabels": {},
                        "mode": "System",
                        "osType": "Linux"
                    }
                ],
                "servicePrincipalProfile": {
                    "clientId": "cc89ad53-4923-4323-bf2a-557c4fa1d7d7"
                },
                "addonProfiles": {
                    "httpApplicationRouting": {
                        "enabled": false
                    },
                    "omsagent": {
                        "enabled": true,
                        "config": {
                            "logAnalyticsWorkspaceResourceID": "[parameters('workspaces_defaultworkspace_16169bdf_109c_4eb7_9963_3d01e2352774_weu_externalid')]"
                        }
                    }
                },
                "nodeResourceGroup": "[concat('MC_Secure-DevOps-Tour_', parameters('managedClusters_Secure_devops_aks_name'), '_westeurope')]",
                "enableRBAC": true,
                "networkProfile": {
                    "networkPlugin": "kubenet",
                    "loadBalancerSku": "Standard",
                    "loadBalancerProfile": {
                        "managedOutboundIPs": {
                            "count": 1
                        },
                        "effectiveOutboundIPs": [
                            {
                                "id": "[parameters('publicIPAddresses_56161307_afd6_48b8_9dcd_11dd561b0776_externalid')]"
                            }
                        ]
                    },
                    "podCidr": "10.244.0.0/16",
                    "serviceCidr": "10.0.0.0/16",
                    "dnsServiceIP": "10.0.0.10",
                    "dockerBridgeCidr": "172.17.0.1/16",
                    "outboundType": "loadBalancer"
                },
                "apiServerAccessProfile": {
                    "enablePrivateCluster": false
                }
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters/agentPools",
            "apiVersion": "2020-03-01",
            "name": "[concat(parameters('managedClusters_Secure_devops_aks_name'), '/agentpool')]",
            "dependsOn": [
                "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_Secure_devops_aks_name'))]"
            ],
            "properties": {
                "count": 2,
                "vmSize": "Standard_DS2_v2",
                "osDiskSizeGB": 128,
                "maxPods": 110,
                "type": "VirtualMachineScaleSets",
                "orchestratorVersion": "1.15.11",
                "nodeLabels": {},
                "mode": "System",
                "osType": "Linux"
            }
        }
    ]
}